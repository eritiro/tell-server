<p id="notice"><%= notice %></p>

<p>
  <%= image_tag(@user.picture.url(:original)) %>
</p>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Admin:</strong>
  <%= @user.admin %>
</p>

<p>
  <strong>Events:</strong>
  <ul>
    <% @user.events.each do |event| %>
      <li><%= event.event_type %></li>
    <% end%>
  </ul>

</p>



<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
